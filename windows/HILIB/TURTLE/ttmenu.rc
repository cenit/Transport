IDR_MAIN_MENU MENU 
BEGIN
	POPUP "&Input"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&About this Turtle Framework...", IDM_ABOUT
		MENUITEM SEPARATOR
		MENUITEM "&Edit file...", IDM_EDT
		MENUITEM "&Copy file...", IDM_COPYF
		MENUITEM "&GUI Input Editor", IDM_GUI
		MENUITEM SEPARATOR
		MENUITEM "GUI Editor is Default Input Editor", IDM_EDIT_GUI
		MENUITEM "Text Editor is Default Input Editor", IDM_EDIT_TEXT
	END

	POPUP "&Run"
	BEGIN
		MENUITEM SEPARATOR
#ifdef WIN32
		MENUITEM "&Turtle as 32bit-DLL", IDM_TURTLE
#else
		MENUITEM "&Turtle as 16bit-DLL", IDM_TURTLE
#endif
		MENUITEM "T&urtle as 32bit-EXE", IDM_TURTLED
		MENUITEM SEPARATOR
		MENUITEM "&Muscat", IDM_MUSCAT
		MENUITEM SEPARATOR
		MENUITEM "E&xit", IDM_EXIT
	END

	POPUP "&Output"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&List File", IDM_TEDI
		MENUITEM SEPARATOR
		MENUITEM "&Print file...", IDM_PRINT
		MENUITEM "Printer &Setup...", IDM_PRINT_SETUP
		MENUITEM SEPARATOR
		MENUITEM "&Delete File...", IDM_DELFILE
	END
	POPUP "&Graphics"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&Histogramms...", IDM_HISTO
		MENUITEM SEPARATOR
		POPUP    "&Change Colors"
		BEGIN
			MENUITEM "&Screen Background...", IDM_SCREEN_BACK
			MENUITEM "&Axis Lines...", IDM_AXIS_LINES
			MENUITEM "Axis &Text...", IDM_AXIS_TEXT
			MENUITEM "&Histogram Body...", IDM_HISTO_BODY
			MENUITEM "Histogram &Background...", IDM_HISTO_BACK
			MENUITEM "&Legoplot Body...", IDM_LEGO_BODY
			MENUITEM "&Gridplot Lines...", IDM_GRID_LINES
			MENUITEM "&Contourplot Background...", IDM_CONT_BACK
			MENUITEM "Contourplot Line &1...", IDM_CONT_LINE1
			MENUITEM "Contourplot Line &2...", IDM_CONT_LINE2
		END
		MENUITEM "Set internal &Default Colors", IDM_DEFAULT_COLORS
		MENUITEM "&Get Colors from file...", IDM_GET_COLORS
		MENUITEM "&Save Colors to file...", IDM_SAVE_COLORS
#ifdef WIN32
		MENUITEM SEPARATOR
		MENUITEM "H&plot Output...", IDM_HPLOT
#endif
	END

	MENUITEM "&Stop", IDM_STOP

	POPUP "&Help"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "Online Info via the &Web", IDM_WWW
	END

END

IDR_POPUP_MENU MENU 
BEGIN
	POPUP ""
	BEGIN
		MENUITEM "Print Graphics\tP",IDM_PRINT
		MENUITEM "Print Graphics to File\tF",IDM_PRINT2
