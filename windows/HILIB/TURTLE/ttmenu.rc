		MENUITEM "No Action",IDM_HOME
		MENUITEM SEPARATOR
		MENUITEM "Contour Plot",IDC_CONT
		MENUITEM "Lego Plot",IDC_LEGO
		MENUITEM "Grid Plot",IDC_GRID
		MENUITEM SEPARATOR
		MENUITEM "Vert Log Scale",IDC_LOG
		MENUITEM "Vert Lin Scale",IDC_LIN
	END
END

