IDR_MAIN_MENU MENU 
BEGIN
	POPUP "&Input"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&About this Transport Framework...", IDM_ABOUT
		MENUITEM SEPARATOR
		MENUITEM "&Edit file...", IDM_EDT
		MENUITEM "&Copy file...", IDM_COPYF
		MENUITEM "&Display Parameters...", IDM_DISPAR
		MENUITEM SEPARATOR
		MENUITEM "&Bero Scaling", IDM_BERO
		MENUITEM "&Show Input", IDM_DISPI
		MENUITEM "Beam &Line Inversion", IDM_BLI
		MENUITEM SEPARATOR
		MENUITEM "&GUI Input Editor", IDM_GUI
		MENUITEM "&Fit Sequence Editor", IDM_IFQ
		MENUITEM SEPARATOR
		MENUITEM "GUI Editor is Default Input Editor", IDM_EDIT_GUI
		MENUITEM "Text Editor is Default Input Editor", IDM_EDIT_TEXT
	END

	POPUP "&Run"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&Standard Transport", IDM_TRANS_FORM
		MENUITEM SEPARATOR
		MENUITEM "&Graphic  Transport", IDM_TRANS_GRAF
		MENUITEM SEPARATOR
		POPUP "Computation &Mode"
		BEGIN
