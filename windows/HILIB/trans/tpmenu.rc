			MENUITEM "Use 32-bit-&DLL", IDM_16
#else
			MENUITEM "Use 16-bit-&DLL", IDM_16
#endif
			MENUITEM "Use 32-bit-&EXE", IDM_32
		END
		MENUITEM SEPARATOR
		MENUITEM "E&xit", IDM_EXIT
	END

	POPUP "&Graphic"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&Redraw", IDM_REDRAW
		MENUITEM "&Ellipses (x,x' and y,y')", IDM_ELLIPSES
		MENUITEM "&Ellipse (dl,dp/p)", IDM_ELLIPSEL
		MENUITEM "&Ellipses (x,y and x',y')", IDM_ELLIPSE2
		MENUITEM "&Second Order", IDM_SECOND
		MENUITEM "Sa&ve Run", IDM_SAVER
	END

	POPUP "&Tables"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&Initial Beam", IDM_BEAM
		MENUITEM "&Quadrupoles", IDM_QUADS
		MENUITEM "Bending &Magnets", IDM_BENDS
		MENUITEM "&Fit Conditions", IDM_FITS
		MENUITEM "&Shifts and Slits", IDM_SHIFTS
		MENUITEM "&Beamline", IDM_BEAMLINE
		MENUITEM "Floor &Layout coordinates", IDM_FLOOR
		MENUITEM SEPARATOR
		MENUITEM "Quad &DACs", IDM_QDAC
		MENUITEM "Quad &Amps", IDM_QAMP
		MENUITEM SEPARATOR
		MENUITEM "Bend &DACs", IDM_BDAC
		MENUITEM "Bend &Amps", IDM_BAMP
	END

	POPUP "&Output"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "Sigma &Matrices", IDM_SIGMA
		MENUITEM "&List File", IDM_TEDI
		MENUITEM SEPARATOR
		MENUITEM "&Tijk Matrix...", IDM_TIJK
		MENUITEM "&Beam Info...", IDM_BEAMINFO
                MENUITEM "&Elements", IDM_ELEMENTS
		MENUITEM SEPARATOR
		MENUITEM "&Print file...", IDM_PRINT
		MENUITEM "Printer &Setup...", IDM_PRINT_SETUP
		MENUITEM "Line &width for Print ...",IDM_PRINT_LW
		MENUITEM SEPARATOR
		MENUITEM "Delete Scratch &Files", IDM_DELSCRA
		MENUITEM "&Delete File...", IDM_DELFILE
	END

	POPUP "O->I &Func"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "&Quad Fields", IDM_QOI
		MENUITEM "&Beam Parameters", IDM_BOI
		MENUITEM "&Element Length", IDM_LOI
		MENUITEM SEPARATOR
		MENUITEM "&Get DACs...", IDM_GETDACS
		MENUITEM "FxyDac   (FOR022.DAT)", IDM_FXYDAC
		MENUITEM "FxyGauss (FOR022.DAT)", IDM_FXYGAU
	END

	POPUP "&Help"
	BEGIN
		MENUITEM SEPARATOR
		MENUITEM "Online Info via the &Web", IDM_WWW
	END
END

